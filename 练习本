option

AJAX
var xhr = new XMLHttpRequest(); //0
xhr.addEventListener("readystatechange", function(cb){
	if(xhr.readyState == 4){
		if(xhr.status >= 200 && xhr.status < 300 || xhr.status == 304){
			cb(xhr.responseText)
		}
	}
});
xhr.open("get", "xxx.json", true); //1
xhr.setRequestHeader('Content-type', 'xxxx'); //2
xhr.send()

//status == 3 开始接收