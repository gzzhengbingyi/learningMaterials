<script>
if(!('remove' in Element.prototype))
    Element.prototype.remove = function() {
        if(this.parentNode) {
            this.parentNode.removeChild(this);
        }
    };

var _ = RGUI._;
var $$ = function(selector){return document.querySelectorAll(selector)};

// var themeSelect = new RGUI.Select2({
